{
    "name": "docs-core-object-page",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/object-page",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/object-page/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-object-page"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/object-page/docs.json",
                "entities": [
                    "DynamicPageComponent",
                    "DynamicPageSubheaderComponent",
                    "DynamicPageHeaderComponent",
                    "DynamicPageTitleContentComponent",
                    "DynamicPageLayoutActionsComponent",
                    "DynamicPageGlobalActionsComponent",
                    "DynamicPageFooterComponent",
                    "DynamicPageContentComponent"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
