{
    "name": "docs-core-layout-panel",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/layout-panel",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/layout-panel/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-layout-panel"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/layout-panel/docs.json",
                "entities": [
                    "LayoutPanelComponent",
                    "LayoutPanelActionsComponent",
                    "LayoutPanelBodyComponent",
                    "LayoutPanelDescriptionComponent",
                    "LayoutPanelFiltersComponent",
                    "LayoutPanelFooterComponent",
                    "LayoutPanelHeaderComponent",
                    "LayoutPanelHeadComponent",
                    "LayoutPanelTitleDirective"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
