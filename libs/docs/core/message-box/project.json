{
    "name": "docs-core-message-box",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/message-box",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/message-box/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-message-box"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/message-box/docs.json",
                "entities": [
                    "MessageBoxRef",
                    "MessageBoxConfig",
                    "MessageBoxContent",
                    "MessageBoxService",
                    "MessageBoxComponent",
                    "MessageBoxBodyComponent",
                    "MessageBoxHeaderComponent",
                    "MessageBoxFooterComponent",
                    "MessageBoxDefaultComponent",
                    "MessageBoxSemanticIconComponent",
                    "MessageBoxContainerComponent"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
