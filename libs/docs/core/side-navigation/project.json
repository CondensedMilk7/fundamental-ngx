{
    "name": "docs-core-side-navigation",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/side-navigation",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/side-navigation/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-side-navigation"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/side-navigation/docs.json",
                "entities": [
                    "SideNavigationComponent",
                    "SideNavigationModel",
                    "SideNavigationUtilityDirective",
                    "SideNavigationMainComponent",
                    "NestedListDirective",
                    "NestedListPopoverComponent",
                    "NestedListHeaderDirective",
                    "NestedListIconComponent",
                    "NestedListItem",
                    "NestedListModel",
                    "NestedListLink",
                    "NestedItemService"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
