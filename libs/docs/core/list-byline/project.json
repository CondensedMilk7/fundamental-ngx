{
    "name": "docs-core-list-byline",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/list-byline",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/list-byline/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-list-byline"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/list-byline/docs.json",
                "entities": [
                    "ListComponent",
                    "ListItemDirective",
                    "ListLabelDirective",
                    "ListTitleDirective",
                    "ListSecondaryDirective",
                    "ListGroupHeaderDirective",
                    "ListIconDirective",
                    "ListFooterDirective",
                    "ListMessageDirective",
                    "ListNavigationItemArrowDirective",
                    "ListNavigationItemTextDirective",
                    "ListNavigationItemComponent"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
