{
    "name": "docs-core-dialog",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/dialog",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/dialog/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-dialog"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/dialog/docs.json",
                "entities": [
                    "DialogService",
                    "DialogContainerComponent",
                    "DialogTitleDirective",
                    "DialogCloseButtonDirective",
                    "DialogDecisiveButtonDirective",
                    "DialogConfig",
                    "DialogPosition",
                    "DialogBodyComponent",
                    "DialogHeaderComponent",
                    "DialogFooterComponent",
                    "DialogRef"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
