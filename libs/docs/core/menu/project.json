{
    "name": "docs-core-menu",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/menu",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/menu/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-menu"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/menu/docs.json",
                "entities": [
                    "MenuAddonDirective",
                    "MenuComponent",
                    "MenuGroupComponent",
                    "MenuItemDirective",
                    "MenuItemAddonDirective",
                    "MenuListDirective",
                    "MenuTitleDirective",
                    "MenuKeyboardService"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
