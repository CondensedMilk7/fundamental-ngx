{
    "name": "docs-core-timeline",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/timeline",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/timeline/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-timeline"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/timeline/docs.json",
                "entities": [
                    "TimelineComponent",
                    "TimelineNodeDefDirective",
                    "TimelineNodeOutletDirective",
                    "TimelineNodeBodyComponent",
                    "TimelineNodeComponent",
                    "TimelineNodeHeaderInfoTitleDirective",
                    "TimelineNodeHeaderInfoTextDirective",
                    "TimelineNodeActionDirective",
                    "TimelineHeaderInfoSubTitleDirective"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
