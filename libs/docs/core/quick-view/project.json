{
    "name": "docs-core-quick-view",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/core/quick-view",
    "projectType": "library",
    "prefix": "fundamental-ngx",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/core/quick-view/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-core-quick-view"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/core/quick-view/docs.json",
                "entities": [
                    "QuickViewComponent",
                    "QuickViewTitleComponent",
                    "QuickViewSubheaderComponent",
                    "QuickViewSubheaderTitleComponent",
                    "QuickViewSubheaderSubtitleComponent",
                    "QuickViewGroupComponent",
                    "QuickViewGroupTitleComponent",
                    "QuickViewGroupItemComponent",
                    "QuickViewGroupItemLabelComponent",
                    "QuickViewGroupItemContentComponent",
                    "QuickViewGroupItemContentElementDirective"
                ]
            },
            "outputs": ["{options}.outputPath"],
            "dependsOn": ["core:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
