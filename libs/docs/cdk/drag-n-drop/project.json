{
    "name": "docs-cdk-dnd",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/cdk/drag-n-drop",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e-noop": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eTsConfig": "libs/docs/cdk/drag-n-drop/e2e/tsconfig.json",
                "baseUrl": "http://localhost:4200",
                "configFile": "libs/docs/cdk/drag-n-drop/wdio.conf.js"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-cdk-dnd"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/cdk/drag-n-drop/docs.json",
                "entities": [
                    "DndListDirective",
                    "DndItemDirective",
                    "DndKeyboardGroupDirective",
                    "DndKeyboardItemDirective"
                ]
            },
            "dependsOn": ["cdk:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
