{
    "name": "docs-platform-table",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/platform/table",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/platform/table/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-platform-table"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/platform/table/docs.json",
                "entities": [
                    "TableComponent",
                    "TableColumnComponent",
                    "TableToolbarComponent",
                    "TableToolbarActionsComponent",
                    "FdpTableCell",
                    "FdpCellDef",
                    "FdpTableHeader",
                    "FdpHeaderCellDef",
                    "TableViewSettingsDialogComponent",
                    "TableViewSettingsFilterComponent",
                    "FdpViewSettingsFilterCustomDef",
                    "TableP13DialogComponent",
                    "TableP13SortComponent",
                    "TableP13FilterComponent",
                    "TableP13GroupComponent",
                    "TableP13ColumnsComponent",
                    "TableDataSource",
                    "TableDataProvider",
                    "PlatformTableCellResponsiveDirective"
                ]
            },
            "dependsOn": ["platform:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
