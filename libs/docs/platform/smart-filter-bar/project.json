{
    "name": "docs-platform-smart-filter-bar",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/platform/smart-filter-bar",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/platform/smart-filter-bar/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-platform-smart-filter-bar"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/platform/smart-filter-bar/docs.json",
                "entities": [
                    "SmartFilterBarComponent",
                    "SmartFilterBarConditionFieldComponent",
                    "SmartFilterBarConditionsDialogComponent",
                    "SmartFilterBarFieldDefinitionDirective",
                    "SmartFilterBarService",
                    "SmartFilterBarSubjectDirective",
                    "SmartFilterBarOptionsDataProvider",
                    "SmartFilterBarSettingsDialogComponent"
                ]
            },
            "dependsOn": ["platform:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
