{
    "name": "docs-platform-icon-tab-bar",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/platform/icon-tab-bar",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/platform/icon-tab-bar/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-platform-icon-tab-bar"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/platform/icon-tab-bar/docs.json",
                "entities": [
                    "IconTabBarComponent",
                    "IconTabBarTextTypeComponent",
                    "IconTabBarIconTypeComponent",
                    "IconTabBarProcessTypeComponent",
                    "IconTabBarFilterTypeComponent",
                    "IconTabBarPopoverComponent",
                    "TextTypePopoverComponent"
                ]
            },
            "dependsOn": ["platform:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
