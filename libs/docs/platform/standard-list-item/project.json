{
    "name": "docs-platform-standard-list-item",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/platform/standard-list-item",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/platform/standard-list-item/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-platform-standard-list-item"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/platform/standard-list-item/docs.json",
                "entities": [
                    "ListComponent",
                    "StandardListItemComponent",
                    "ListFooter",
                    "ListGroupHeader",
                    "ListConfig"
                ]
            },
            "dependsOn": ["platform:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
