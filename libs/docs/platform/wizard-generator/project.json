{
    "name": "docs-platform-wizard-generator",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/docs/platform/wizard-generator",
    "projectType": "library",
    "prefix": "fdp-doc",
    "targets": {
        "e2e": {
            "executor": "@fundamental-ngx/nx-plugin:e2e-test",
            "options": {
                "e2eFiles": ["libs/docs/platform/wizard-generator/e2e/**/*.e2e-spec.ts"],
                "devServerTarget": "docs:serve:e2e"
            },
            "outputs": ["{workspaceRoot}/allure-results/docs-platform-wizard-generator"]
        },
        "extract-compodoc": {
            "executor": "@fundamental-ngx/nx-plugin:compodoc",
            "options": {
                "outputPath": "libs/docs/platform/wizard-generator/docs.json",
                "entities": [
                    "WizardGeneratorService",
                    "WizardDialogGeneratorService",
                    "WizardGeneratorComponent",
                    "DialogWizardGeneratorComponent",
                    "WizardBodyComponent",
                    "WizardGeneratorStepComponent",
                    "WizardSummaryStepComponent",
                    "WizardSummarySectionComponent"
                ]
            },
            "dependsOn": ["platform:compodoc"]
        }
    },
    "tags": ["type:lib", "scope:docs"]
}
